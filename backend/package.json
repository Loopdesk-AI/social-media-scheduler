{
  "name": "social-media-backend",
  "version": "1.0.0",
  "description": "Social media integration backend with Express and BullMQ",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "prisma:generate": "prisma generate --schema=./src/database/prisma/schema.prisma",
    "prisma:push": "prisma db push --schema=./src/database/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./src/database/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./src/database/prisma/schema.prisma",
    "db:seed": "ts-node src/database/seed.ts"
  },
  "keywords": [
    "social-media",
    "instagram",
    "youtube",
    "scheduler"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@bull-board/api": "^6.14.2",
    "@bull-board/express": "^6.14.2",
    "@prisma/client": "^6.5.0",
    "@sentry/node": "^10.25.0",
    "@sentry/profiling-node": "^10.25.0",
    "axios": "^1.7.7",
    "bcryptjs": "^3.0.3",
    "bullmq": "^5.12.12",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "googleapis": "^137.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "prom-client": "^15.1.3",
    "sharp": "^0.33.4",
    "twitter-api-v2": "^1.15.2",
    "uuid": "^13.0.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.9",
    "prisma": "^6.5.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
